{
  "printWidth": 120,
  "tabWidth": 2,
  "plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "tailwindConfig": "./tailwind.config.js",
  "tailwindFunctions": ["clsx", "cn"],
  "importOrder": [
    "<TYPES>",
    "^(react/.*$)|^(react.*$)",
    "^(expo/.*$)|^(expo.*$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "<TYPES>^@/.*$",
    "^@/.*$",
    "",
    "<TYPES>^[.]",
    "^[.]"
  ],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"]
}
